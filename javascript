ADD CSS COM JS
	
	adicionando : 
	$('#collapse-navbar').on('show.bs.collapse', function(){
		$('.topCasaFina-banner').css('transform', 'translate(-50%, 10%)');
	});

	removendo : 
	$('#collapse-navbar').on('hide.bs.collapse', function(){
		$('.topCasaFina-banner').css('transform', 'translate(-50%, -50%)');
	});;

